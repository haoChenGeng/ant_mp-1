.lease-description {
  font-family: PingFangSC-Regular;
  width: 100%;
  background-color: #fff;
  padding: 0 24px;
  box-sizing: border-box
}
.lease-description view.span {
  display: initial;
}

.lease-description .description-item {
  padding: 24px 0
}

.lease-description .description-item .item-title {
  font-family: PingFangSC-Medium;
  color: #111;
  font-size: 18px;
  line-height: 25px;
  font-weight: 700
}

.lease-description .description-item .item-con {
  margin-top: 16px
}

.lease-description .advantage .adv-title {
  height: 28px;
  line-height: 28px;
  background: #f7f7f7;
  text-align: center
}

.lease-description .advantage .adv-title .span {
  font-size: 14px;
  color: #111;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1
}

.lease-description .advantage .adv-compare {
  width: 45%
}

.lease-description .advantage .adv-compare .compare-txt {
  font-size: 12px;
  color: #888;
  line-height: 17px
}

.lease-description .advantage .adv-compare .compare-txt .span {
  color: #111;
  word-wrap: break-word
}

.lease-description .advantage .adv-compare .compare-price .price {
  font-size: 30px;
  font-family: PingFangSC-Light;
  color: #ff4754;
  line-height: 42px;
  word-wrap: break-word
}

.lease-description .advantage .adv-compare .compare-price .unit {
  font-size: 20px;
  font-family: PingFangSC-Light;
  color: #bbb;
  line-height: 42px
}

.lease-description .advantage .adv-vs {
  width: 36px;
  height: 36px;
  line-height: 36px;
  background: #f7f7f7;
  border-radius: 50%;
  text-align: center;
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0
}

.lease-description .advantage .adv-vs .span {
  font-size: 16px;
  font-family: PingFangSC-Medium;
  color: #bbb
}

.lease-description .advantage .adv-compare-con {
  position: relative;
  width: 100%;
  margin-top: 10px
}

.lease-description .advantage .adv-compare-con .compare-left {
  float: left
}

.lease-description .advantage .adv-compare-con .compare-right {
  float: right
}

.lease-description .advantage .adv-compare-con .compare-right .compare-txt {
  padding-left: 25px
}

.lease-description .advantage .adv-compare-con .compare-right .compare-price {
  padding-left: 25px
}

.lease-description .advantage .adv-compare-con .compare-right .compare-price .price {
  color: #111
}

.lease-description .maturity view.ul view.li {
  font-size: 12px;
  color: #111;
  line-height: 25px
}

.lease-description .process .bg-img {
  width: 100%;
  min-height: 110px;
  height: auto;
  background-image: url(/image/GoodDetail/LeaseDescription/stepTT1x.png);
  background-repeat: no-repeat;
  background-size: contain
}

@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
  .lease-description .process .bg-img {
    background-image: url(/image/GoodDetail/LeaseDescription/stepTT1.5x.png);
    background-repeat: no-repeat;
    background-size: contain
  }
}

.lease-description .process .process-bg {
  display: block;
  margin: auto;
  width: 100%;
  max-width: 330px;
  height: auto
}

.lease-description .process .item-box {
  width: 96px;
  height: 44px;
  line-height: 44px;
  display: inline-block;
  background: #f5f5f5;
  border-radius: 4px;
  text-align: center
}

.lease-description .process .item-box .span {
  font-size: 14px;
  color: #111
}

.lease-description .process .box-arrow {
  color: #eee
}

.lease-description .rules .rules-txt {
  font-size: 14px;
  color: #111;
  line-height: 21px
}

.lease-description .rules .rules-txt .span {
  font-size: 11px
}

.lease-description .rules .rules-sample {
  margin-top: 19px;
  font-size: 12px;
  color: #888;
  line-height: 18px
}

.lease-description .rules .rules-tip {
  font-size: 12px;
  height: 33px;
  line-height: 33px;
  background: #f5f5f5;
  text-align: left;
  padding-left: 12px;
  margin-top: 16px
}

.lease-description .rules .rules-tip .tip-title {
  color: #FFBA00
}

.lease-description .rules .rules-tip .tip-txt {
  color: #111
}

.lease-description .rules .rules-img {
  margin-top: 16px
}

.lease-description .rules .rules-img .bg-rules-img {
  width: 100%;
  min-height: 140px;
  height: auto;
  background-image: url(/image/GoodDetail/LeaseDescription/dayTT1x.png);
  background-repeat: no-repeat;
  background-size: contain
}

@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
  .lease-description .rules .rules-img .bg-rules-img {
    background-image: url(/image/GoodDetail/LeaseDescription/dayTT1.5x.png);
    background-repeat: no-repeat;
    background-size: contain
  }
}

.lease-description .rules .rules-img img {
  display: block;
  margin: auto;
  width: 100%;
  max-width: 330px;
  height: auto
}

@media screen and (max-width:320px) {
  .lease-description .advantage .adv-compare-con .compare-right .compare-txt {
    padding-left: 21px
  }
  .lease-description .advantage .adv-compare-con .compare-right .compare-price {
    padding-left: 21px
  }
  .lease-description .advantage .adv-compare-con .compare-right .compare-price .price {
    font-size: 26px
  }
  .lease-description .process .bg-img {
    min-height: 82px
  }
  .lease-description .rules .rules-img .bg-rules-img {
    min-height: 130px
  }
}
